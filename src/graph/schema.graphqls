# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Notification {
  id: ID!
  title: String!
  body: String!
}

type Query {
  notifications: [Notification!]!
}

input NotificationInput {
  title: String!
  body: String!
}

type Mutation {
  createNotification(input: NotificationInput!): Notification!
}
